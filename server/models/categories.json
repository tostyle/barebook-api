  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "thai_name": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "packages": {
      "type": "hasMany",
      "model": "packages",
      "foreignKey": "categoryId",
      "options": {
        "nestRemoting": false
      }
    }
  },
  "acls": [
